<section id="login-section">
    {{labeled-input label='Login:' value=model.login}}
    {{labeled-input label='Password:' value=model.password}}

    <p class="property-input">
        {{#if errorMessage}}
        <div class="error-message">
            {{errorMessage}}
        </div>
        {{/if}}
        <button {{action "logIn"}}>
            Entrar
        </button>
    </p>
</section>